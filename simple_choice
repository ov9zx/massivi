#include <iostream>
using namespace std;

int main() {
    int* a = new int[7];

    // --- ВВОД -----------------------------------
    for (int i = 0; i < 7; i++) {
        cin >> a[i];
    }

    // --- СОРТИРОВКА ВЫБОРОМ ---------------------
    for (int i = 0; i < 7; i++) {

        int minIndex = i; // считаем, что минимум здесь

        // ищем настоящий минимум
        for (int j = i + 1; j < 7; j++) {
            if (a[j] < a[minIndex]) {
                minIndex = j;
            }
        }

        // обмен
        int box = a[i];
        a[i] = a[minIndex];
        a[minIndex] = box;
    }

    // --- ВЫВОД ----------------------------------
    for (int i = 0; i < 7; i++) {
        cout << a[i] << " ";
    }

    delete[] a;
    return 0;
}
